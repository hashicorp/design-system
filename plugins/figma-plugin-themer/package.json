{
    "name": "hds-themer",
    "version": "1.0.0",
    "description": "Figma plugin that swaps HDS themes [fork/customization of thomas-lowry/themer]",
    "license": "UNLICENSED",
    "author": "",
    "main": "dist/script.js",
    "targets": {
        "main": false,
        "app": {
            "distDir": "dist"
        }
    },
    "browserslist": [
        "last 1 Chrome version"
    ],
    "scripts": {
        "typecheck": "tsc --noEmit",
        "build": "rollup --config rollup.config.js",
        "watch": "rollup --config rollup.config.js --watch",
        "format:check": "prettier --check \"src/**/*.{ts, tsx}\"",
        "lint:check": "eslint \"src/**/*.{js,ts,tsx}\"",
        "verify": "yarn format:check && yarn lint:check"
    },
    "devDependencies": {
        "@figma/plugin-typings": "^1.42.1",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@rollup/plugin-replace": "^3.0.1",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "@typescript-eslint/parser": "^5.10.2",
        "classnames": "^2.3.1",
        "eslint": "^8.8.0",
        "eslint-plugin-prettier": "^4.0.0",
        "lodash": "^4.17.21",
        "postcss": "^8.4.6",
        "postcss-import": "^14.0.2",
        "prettier": "^2.5.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-figma-ui": "^1.1.0",
        "rollup": "^2.67.0",
        "rollup-plugin-html-bundle": "^0.0.3",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-scss": "^3.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.31.2",
        "sass": "^1.49.7",
        "tailwindcss": "^3.0.18",
        "typescript": "^4.5.5"
    },
    "dependencies": {}
}
