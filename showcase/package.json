{
  "name": "showcase",
  "version": "0.0.0",
  "private": true,
  "description": "Components showcase",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashicorp/design-system.git",
    "directory": "showcase"
  },
  "license": "MPL-2.0",
  "author": "HashiCorp Design Systems <design-systems@hashicorp.com>",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "yarn build:packages && ember build --environment=production",
    "build:packages": "yarn workspace @hashicorp/ember-flight-icons build && yarn workspace @hashicorp/design-system-components build",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:css": "stylelint \"app/styles/**/*.scss\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "yarn build:packages && ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "yarn build:packages && ember test",
    "test:ember-compatibility": "ember try:each",
    "test:ember:percy": "percy exec ember test",
    "test:a11y": "ENABLE_A11Y_AUDIT=true ember test --server",
    "test:a11y-report": "ENABLE_A11Y_MIDDLEWARE_REPORTER=true ember test"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.0",
    "@ember/test-waiters": "^3.1.0",
    "@embroider/test-setup": "^3.0.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.4.0",
    "@glint/environment-ember-loose": "^1.4.0",
    "@glint/template": "^1.4.0",
    "@hashicorp/design-system-components": "workspace:^",
    "@hashicorp/design-system-tokens": "workspace:^",
    "@hashicorp/ember-flight-icons": "workspace:^",
    "@percy/cli": "^1.27.3",
    "@percy/ember": "^4.2.0",
    "@tsconfig/ember": "^3.0.6",
    "@types/qunit": "^2.19.10",
    "@types/rsvp": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.1",
    "ember-a11y-testing": "^6.1.1",
    "ember-auto-import": "^2.6.3",
    "ember-basic-dropdown": "^8.1.0",
    "ember-body-class": "^3.0.0",
    "ember-cli": "~5.3.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-deprecation-workflow": "^2.1.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-string-helpers": "^6.1.0",
    "ember-cli-terser": "^4.0.2",
    "ember-concurrency": "^4.0.2",
    "ember-deep-tracked": "^2.0.0",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^4.1.0",
    "ember-page-title": "^8.0.0",
    "ember-power-select": "^8.2.0",
    "ember-qunit": "^8.0.1",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.3.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-style-modifier": "^4.3.1",
    "ember-template-lint": "^4.18.2",
    "ember-template-lint-plugin-prettier": "^4.2.0",
    "ember-truth-helpers": "^3.1.1",
    "ember-try": "^3.0.0",
    "ember-unique-id-helper-polyfill": "^1.2.2",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^8.0.1",
    "loader.js": "^4.7.0",
    "prettier": "^2.8.8",
    "qunit": "^2.20.0",
    "qunit-dom": "^3.0.0",
    "sass": "^1.69.5",
    "sinon": "^17.0.0",
    "stylelint": "^16.3.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard-scss": "^13.1.0",
    "tracked-built-ins": "^3.2.0",
    "typescript": "^5.4.2",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  }
}
