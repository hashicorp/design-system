{
  "name": "showcase",
  "version": "0.0.0",
  "private": true,
  "description": "Components showcase",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashicorp/design-system.git",
    "directory": "showcase"
  },
  "license": "MPL-2.0",
  "author": "HashiCorp Design Systems <design-systems@hashicorp.com>",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "pnpm build:packages && ember build --environment=production",
    "build:packages": "pnpm -F @hashicorp/design-system-components build",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.scss\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "pnpm build:packages && ember serve",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\"",
    "test:ember": "pnpm build:packages && ember test",
    "test:ember-compatibility": "ember try:each",
    "test:ember:percy": "percy exec ember test",
    "test:a11y": "pnpm build:packages && ENABLE_A11Y_AUDIT=true ember test --server -f=\"Acceptance\"",
    "test:a11y-report": "pnpm build:packages && ENABLE_A11Y_AUDIT=true ENABLE_A11Y_MIDDLEWARE_REPORTER=true ember test -f=\"Acceptance\""
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@codemirror/state": "^6.5.0",
    "@ember/optional-features": "^2.2.0",
    "@ember/render-modifiers": "^2.1.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^4.0.4",
    "@ember/test-waiters": "^3.1.0",
    "@embroider/test-setup": "^3.0.3",
    "@eslint/js": "^9.27.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.5.2",
    "@glint/environment-ember-loose": "^1.5.2",
    "@glint/environment-ember-template-imports": "^1.5.2",
    "@glint/template": "^1.5.2",
    "@hashicorp/design-system-components": "workspace:*",
    "@hashicorp/design-system-tokens": "workspace:*",
    "@percy/cli": "^1.30.5",
    "@percy/ember": "^4.2.0",
    "@tsconfig/ember": "^3.0.10",
    "@types/qunit": "^2.19.12",
    "@types/rsvp": "^4.0.9",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.1.2",
    "ember-a11y-testing": "^7.1.2",
    "ember-auto-import": "^2.10.0",
    "ember-basic-dropdown": "^8.4.0",
    "ember-body-class": "^3.0.0",
    "ember-cli": "~5.12.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-deprecation-workflow": "^3.0.2",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-string-helpers": "^6.1.0",
    "ember-cli-string-utils": "^1.1.0",
    "ember-cli-terser": "^4.0.2",
    "ember-concurrency": "^4.0.2",
    "ember-deep-tracked": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-math-helpers": "^3.0.0",
    "ember-modifier": "^4.2.0",
    "ember-page-title": "^8.2.3",
    "ember-power-select": "^8.6.2",
    "ember-qunit": "^9.0.1",
    "ember-resolver": "^13.1.0",
    "ember-router-generator": "^2.0.0",
    "ember-set-helper": "^3.0.1",
    "ember-source": "~5.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-style-modifier": "^4.4.0",
    "ember-template-imports": "^4.3.0",
    "ember-template-lint": "^7.7.0",
    "ember-truth-helpers": "^4.0.3",
    "ember-try": "^3.0.0",
    "ember-unique-id-helper-polyfill": "^1.2.2",
