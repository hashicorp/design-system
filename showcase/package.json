{
  "name": "showcase",
  "version": "0.0.0",
  "private": true,
  "description": "Components showcase",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashicorp/design-system.git",
    "directory": "showcase"
  },
  "license": "MPL-2.0",
  "author": "HashiCorp Design Systems <design-systems@hashicorp.com>",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:css": "stylelint \"app/styles/**/*.scss\"",
    "lint:css:fix": "npm-run-all \"lint:css --fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:ember:percy": "percy exec ember test",
    "test:a11y": "ENABLE_A11Y_AUDIT=true ember test --server",
    "test:a11y-report": "ENABLE_A11Y_MIDDLEWARE_REPORTER=true ember test"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.0",
    "@ember/test-waiters": "^3.1.0",
    "@embroider/test-setup": "^3.0.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.2.1",
    "@glint/environment-ember-loose": "^1.2.1",
    "@glint/template": "^1.2.1",
    "@hashicorp/design-system-components": "workspace:^",
    "@hashicorp/design-system-tokens": "workspace:^",
    "@hashicorp/ember-flight-icons": "workspace:^",
    "@percy/cli": "^1.27.3",
    "@percy/ember": "^4.2.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-a11y-testing": "^6.1.1",
    "ember-auto-import": "^2.6.3",
    "ember-body-class": "^3.0.0",
    "ember-cli": "~5.3.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-deprecation-workflow": "^2.1.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-string-helpers": "^6.1.0",
    "ember-cli-terser": "^4.0.2",
    "ember-concurrency": "^3.1.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.0.0",
    "ember-power-select": "^7.1.2",
    "ember-qunit": "^8.0.1",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.3.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-style-modifier": "^3.0.1",
    "ember-template-lint": "^4.18.2",
    "ember-template-lint-plugin-prettier": "^4.2.0",
    "ember-truth-helpers": "^3.1.1",
    "ember-try": "^3.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^8.0.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "qunit": "^2.20.0",
    "qunit-dom": "^3.0.0",
    "sass": "^1.69.5",
    "sinon": "^17.0.0",
    "stylelint": "^15.11.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard-scss": "^11.1.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  }
}
