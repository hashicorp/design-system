{
  "name": "showcase",
  "version": "0.0.0",
  "private": true,
  "description": "Components showcase",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashicorp/design-system.git",
    "directory": "showcase"
  },
  "license": "MPL-2.0",
  "author": "HashiCorp Design Systems <design-systems@hashicorp.com>",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "yarn build:packages && ember build --environment=production",
    "build:packages": "yarn workspace @hashicorp/design-system-components build",
    "lint": "concurrently \"yarn:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"yarn:lint:*:fix\" --names \"fix:\"",
    "lint:css": "stylelint \"app/styles/**/*.scss\"",
    "lint:css:fix": "concurrently \"yarn:lint:css --fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "yarn build:packages && ember serve",
    "test": "concurrently \"yarn:lint\" \"yarn:test:*\" --names \"lint,test:\"",
    "test:ember": "yarn build:packages && ember test",
    "test:ember-compatibility": "ember try:each",
    "test:ember:percy": "percy exec ember test",
    "test:a11y": "yarn build:packages && ENABLE_A11Y_AUDIT=true ember test --server",
    "test:a11y-report": "yarn build:packages && ENABLE_A11Y_AUDIT=true ENABLE_A11Y_MIDDLEWARE_REPORTER=true ember test"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^4.0.4",
    "@ember/test-waiters": "^3.1.0",
    "@embroider/test-setup": "^3.0.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.5.0",
    "@glint/environment-ember-loose": "^1.4.0",
    "@glint/environment-ember-template-imports": "^1.5.0",
    "@glint/template": "^1.5.0",
    "@hashicorp/design-system-components": "workspace:^",
    "@hashicorp/design-system-tokens": "workspace:^",
    "@percy/cli": "^1.30.5",
    "@percy/ember": "^4.2.0",
    "@tsconfig/ember": "^3.0.8",
    "@types/qunit": "^2.19.12",
    "@types/rsvp": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.1.0",
    "ember-a11y-testing": "^7.0.2",
    "ember-auto-import": "^2.10.0",
    "ember-basic-dropdown": "^8.4.0",
    "ember-body-class": "^3.0.0",
    "ember-cli": "~5.9.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-deprecation-workflow": "^3.0.2",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-string-helpers": "^6.1.0",
    "ember-cli-terser": "^4.0.2",
    "ember-concurrency": "^4.0.2",
    "ember-deep-tracked": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^4.1.0",
    "ember-page-title": "^8.2.3",
    "ember-power-select": "^8.6.0",
    "ember-qunit": "^8.1.1",
    "ember-resolver": "^13.1.0",
    "ember-set-helper": "^3.0.1",
    "ember-source": "~5.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-style-modifier": "^4.4.0",
    "ember-template-imports": "^4.2.0",
    "ember-template-lint": "^6.0.0",
    "ember-template-lint-plugin-prettier": "^5.0.0",
    "ember-truth-helpers": "^4.0.3",
    "ember-try": "^3.0.0",
    "ember-unique-id-helper-polyfill": "^1.2.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.3.3",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-qunit": "^8.1.2",
    "loader.js": "^4.7.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-ember-template-tag": "^2.0.4",
    "qunit": "^2.23.1",
    "qunit-dom": "^3.4.0",
    "sass": "^1.69.5",
    "sinon": "^19.0.2",
    "stylelint": "^16.12.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard-scss": "^14.0.0",
    "tracked-built-ins": "^3.4.0",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  }
}
