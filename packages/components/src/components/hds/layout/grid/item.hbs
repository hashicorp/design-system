{{#if (eq this.componentTag "div")}}
  <div class={{this.classNames}} ...attributes>{{yield}}</div>
{{else}}
  {{#let (element this.componentTag) as |Tag|}}
    <Tag class={{this.classNames}} ...attributes>
      {{yield}}
    </Tag>
  {{/let}}
{{/if}}