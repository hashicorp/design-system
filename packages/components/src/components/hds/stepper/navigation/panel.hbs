<section
  class={{this.classNames}}
  ...attributes
  role="tabpanel"
  id={{this._panelId}}
  hidden={{not this.isVisible}}
  aria-labelledby={{this.coupledStepId}}
  {{did-insert this.didInsertNode}}
>
  {{#if this.isVisible}}
    {{yield}}
  {{/if}}
</section>