{{page-title "Pagination Component"}}

{{log this.consumerRouteName }}
{{log this.consumerQueryFunction }}
<pre>this.consumerRouteName {{this.consumerRouteName}}</pre>
<pre>this.consumerQueryFunction {{this.consumerQueryFunction}}</pre>

<Hds::Pagination::Numbered
  @totalItems={{40}}
  @itemsPerPage={{10}}
  @pageSizes={{array 10 30 50}}
  @route={{this.consumerRouteName}}
  @queryFunction={{this.consumerQueryFunction}}
  @onPageChange={{this.handlePageChange}}
  @onPageSizeChange={{this.handlePageSizeChange}}
/>
