<pre>this._currentPage {{this._currentPage}}</pre>
<pre>this.currentPage {{this.currentPage}}</pre>
<pre>this.currentPageSize {{this.currentPageSize}}</pre>
<pre>this._currentPageSize {{this._currentPageSize}}</pre>
<pre>queryParams[demoCurrentPage] {{this.routeQueryParams.demoCurrentPage}}</pre>
<pre>queryParams[demoCurrentPageSize] {{this.routeQueryParams.demoCurrentPageSize}}</pre>

<div class="hds-pagination" ...attributes>
  {{#if this.showInfo}}
    <Hds::Pagination::Info
      @itemsRangeStart={{this.itemsRangeStart}}
      @itemsRangeEnd={{this.itemsRangeEnd}}
      @totalItems={{@totalItems}}
      @showTotalItems={{@showTotalItems}}
    />
  {{/if}}

  <nav class="hds-pagination-nav" aria-label="Pagination navigation">
    <Hds::Pagination::Nav::Arrow
      @direction="prev"
      @showLabel={{this.showLabels}}
      @route={{@route}}
      @query={{this.templateQueryParams.prev}}
      @model={{@model}}
      @models={{@models}}
      @replace={{@replace}}
      @onClick={{this.onPageChange}}
      @disabled={{this.isDisabledPrev}}
    />
    {{#if this.showPageNumbers}}
      <ul class="hds-pagination-nav__page-list">
        {{#each this.pages as |page|}}
          <li class="hds-pagination-nav__page-item">
            {{#if (eq page "â€¦")}}
              <Hds::Pagination::Nav::Ellipsis />
            {{else}}
              <Hds::Pagination::Nav::Number
                @page={{page}}
                @route={{@route}}
                @query={{get this.templateQueryParams.pages page}}
                @model={{@model}}
                @models={{@models}}
                @replace={{@replace}}
                @onClick={{this.onPageChange}}
                @isSelected={{if (eq page this.currentPage) true false}}
              />
            {{/if}}
          </li>
        {{/each}}
      </ul>
    {{/if}}
    <Hds::Pagination::Nav::Arrow
      @direction="next"
      @showLabel={{this.showLabels}}
      @route={{@route}}
      @query={{this.templateQueryParams.next}}
      @model={{@model}}
      @models={{@models}}
      @replace={{@replace}}
      @onClick={{this.onPageChange}}
      @disabled={{this.isDisabledNext}}
    />
  </nav>

  {{#if this.showSizeSelector}}
    <Hds::Pagination::SizeSelector
      @sizes={{@pageSizes}}
      @selectedSize={{this.currentPageSize}}
      @onChange={{this.onPageSizeChange}}
    />
  {{/if}}
</div>