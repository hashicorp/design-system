<section
  class="hds-tabs__panel"
  ...attributes
  role="tabpanel"
  id={{this.panelId}}
  hidden={{not this.isVisible}}
  aria-labelledby={{this.coupledTabId}}
  {{did-insert this.didInsertNode}}
  {{will-destroy this.willDestroyNode}}
>
  {{yield (hash isVisible=this.isVisible)}}
</section>