{
    "name": "structure-to-flight-icons",
    "version": "1.0.0",
    "description": "Figma plugin that replaces Structure icons with Flight icons",
    "license": "UNLICENSED",
    "author": "",
    "main": "dist/script.js",
    "targets": {
        "main": false,
        "app": {
            "distDir": "dist"
        }
    },
    "browserslist": [
        "last 1 Chrome version"
    ],
    "scripts": {
        "typecheck": "tsc --noEmit",
        "build": "rollup --config rollup.config.js",
        "watch": "rollup --config rollup.config.js --watch",
        "format:check": "prettier --check \"src/**/*.{ts, tsx}\"",
        "lint:check": "eslint \"src/**/*.{js,ts,tsx}\"",
        "verify": "yarn format:check && yarn lint:check"
    },
    "devDependencies": {
        "@figma/plugin-typings": "^1.22.0",
        "@rollup/plugin-commonjs": "^15.0.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@rollup/plugin-replace": "^2.4.2",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.1.0",
        "@typescript-eslint/parser": "^4.1.0",
        "eslint": "^7.8.1",
        "eslint-plugin-prettier": "^3.1.4",
        "flight": "https://github.com/hashicorp/flight/",
        "lodash": "^4.17.21",
        "postcss-import": "12.0.1",
        "prettier": "^2.1.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-figma-ui": "^1.0.1",
        "rollup": "^2.26.4",
        "rollup-plugin-html-bundle": "^0.0.3",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-postcss": "^3.1.6",
        "rollup-plugin-scss": "^3.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.27.2",
        "sass": "^1.41.1",
        "tailwindcss": "^1.7.3",
        "typescript": "^4.0.2"
    },
    "dependencies": {}
}
