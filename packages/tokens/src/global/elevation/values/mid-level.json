{
  "elevation": {
    "mid": {
      "shadow-01": {
        "x": {
          "$type": "dimension",
          "$value": "0",
          "unit": "px",
          "private": true
        },
        "y": {
          "$type": "dimension",
          "$value": "2",
          "unit": "px",
          "private": true
        },
        "blur": {
          "$type": "dimension",
          "$value": "3",
          "unit": "px",
          "private": true
        },
        "spread": {
          "$type": "dimension",
          "$value": "0",
          "unit": "px",
          "private": true
        },
        "color": {
          "$type": "color",
          "$value": "{elevation.color.base}",
          "alpha": "0.10",
          "private": true
        }
      },
      "shadow-02": {
        "x": {
          "$type": "dimension",
          "$value": "0",
          "unit": "px",
          "private": true
        },
        "y": {
          "$type": "dimension",
          "$value": "8",
          "unit": "px",
          "private": true
        },
        "blur": {
          "$type": "dimension",
          "$value": "16",
          "unit": "px",
          "private": true
        },
        "spread": {
          "$type": "dimension",
          "$value": "-10",
          "unit": "px",
          "private": true
        },
        "color": {
          "alpha": "0.20",
          "private": true,
          "$value": "{elevation.color.base}",
          "$type": "color"
        }
      },
      "border": {
        "width": {
          "$type": "dimension",
          "$value": "1",
          "unit": "px",
          "private": true
        },
        "color": {
          "$type": "color",
          "$value": "{elevation.color.base}",
          "alpha": "0.15",
          "private": true
        }
      },
      "box-shadow-01": {
        "$value": "{elevation.mid.shadow-01.x} {elevation.mid.shadow-01.y} {elevation.mid.shadow-01.blur} {elevation.mid.shadow-01.spread} {elevation.mid.shadow-01.color}",
        "private": true
      },
      "box-shadow-02": {
        "$value": "{elevation.mid.shadow-02.x} {elevation.mid.shadow-02.y} {elevation.mid.shadow-02.blur} {elevation.mid.shadow-02.spread} {elevation.mid.shadow-02.color}",
        "private": true
      },
      "box-shadow-border": {
        "$value": "0 0 0 {elevation.mid.border.width} {elevation.mid.border.color}",
        "private": true
      }
    }
  }
}