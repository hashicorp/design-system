{
  "elevation": {
    "inset": {
      "shadow-01": {
        "x": {
          "$type": "dimension",
          "$value": "0",
          "unit": "px",
          "private": true
        },
        "y": {
          "$type": "dimension",
          "$value": "1",
          "unit": "px",
          "private": true
        },
        "blur": {
          "$type": "dimension",
          "$value": "2",
          "unit": "px",
          "private": true
        },
        "spread": {
          "$type": "dimension",
          "$value": "1",
          "unit": "px",
          "private": true
        },
        "color": {
          "$type": "color",
          "$value": "{elevation.color.base}",
          "alpha": "0.1",
          "private": true
        }
      },
      "border": {
        "width": {
          "$type": "dimension",
          "$value": "1",
          "unit": "px",
          "private": true
        },
        "color": {
          "$type": "color",
          "$value": "{elevation.color.base}",
          "alpha": "0.30",
          "private": true
        }
      },
      "box-shadow-01": {
        "$value": "inset {elevation.inset.shadow-01.x} {elevation.inset.shadow-01.y} {elevation.inset.shadow-01.blur} {elevation.inset.shadow-01.spread} {elevation.inset.shadow-01.color}",
        "private": true
      },
      "box-shadow-border": {
        "$value": "inset 0 0 0 {elevation.inset.border.width} {elevation.inset.border.color}",
        "private": true
      }
    }
  }
}