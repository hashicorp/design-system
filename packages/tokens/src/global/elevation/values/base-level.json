{
  "elevation": {
    "base": {
      "border": {
        "width": {
          "$type": "dimension",
          "$value": "1",
          "unit": "px",
          "private": true
        },
        "color": {
          "$type": "color",
          "$value": "{elevation.color.base}",
          "alpha": "0.20",
          "private": true
        }
      },
      "box-shadow-border": {
        "$value": "0 0 0 {elevation.base.border.width} {elevation.base.border.color}",
        "private": true
      }
    }
  }
}