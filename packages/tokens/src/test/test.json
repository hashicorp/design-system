{
  "base": {
    "simple": {
      "$type": "color",
      "$value": "#123456",
      "group": "palette",
      "private": "true"
    },
    "aliased": {
      "$type": "color",
      "$value": "{base.simple}",
      "group": "palette"
    },
    "double-aliased": {
      "$type": "color",
      "$value": "{base.aliased}",
      "group": "palette"
    }
  },
  "themed": {
    "simple": {
      "$type": "color",
      "$value": "#ffffff",
      "group": "palette",
      "$modes": {
        "default": "#ffffff",
        "cds-g0": "#ffffff",
        "cds-g10": "#cccccc",
        "cds-g90": "#333333",
        "cds-g100": "#000000"
      }
    },
    "aliased": {
      "$type": "color",
      "$value": "{themed.simple}",
      "group": "palette"
    },
    "mixed": {
      "$type": "color",
      "$value": "{base.aliased}",
      "group": "palette",
      "$modes": {
        "default": "{base.simple}",
        "cds-g0": "{themed.aliased}",
        "cds-g10": "{themed.simple}",
        "cds-g90": "{themed.simple}",
        "cds-g100": "{themed.simple}"
      }
    }
  },
  "alpha": {
    "simple": {
      "$type": "color",
      "$value": "{base.simple}",
      "alpha": "0.4",
      "group": "palette"
    },
    "aliased": {
      "$type": "color",
      "$value": "{base.aliased}",
      "alpha": "0.2",
      "group": "palette"
    },
    "themed": {
      "$type": "color",
      "$value": "#FF0000",
      "alpha": "0.1",
      "group": "palette",
      "$modes": {
        "default": "#00FF00",
        "cds-g0": "#0000FF",
        "cds-g10": "#FFFF00",
        "cds-g90": "#FF00FF",
        "cds-g100": "#00FFFF"
      }
    },
    "aliased-to-themed": {
      "$type": "color",
      "$value": "{themed.simple}",
      "alpha": "0.2",
      "group": "palette"
    },
    "themed-aliases": {
      "$type": "color",
      "$value": "{base.aliased}",
      "alpha": "0.1",
      "group": "palette",
      "$modes": {
        "default": "{base.aliased}",
        "cds-g0": "{base.simple}",
        "cds-g10": "{base.simple}",
        "cds-g90": "{base.simple}",
        "cds-g100": "{base.simple}"
      }
    }
  },
  "chained": {
    "a": {
      "$value": "aaa"
    },
    "b→a": {
      "$value": "{chained.a}"
    },
    "c→b": {
      "$value": "{chained.b→a}",
      "private": true
    },
    "d→b": {
      "$value": "{chained.d→b}"
    },
    "d→c*": {
      "$value": "{chained.c→b}"
    }
  },
  "transformed": {
    "x": {
      "$value": "xxx"
    },
    "y": {
      "$value": "yyy"
    },
    "z": {
      "$value": "zzz"
    },
    "k-themed": {
      "$value": "kkk",
      "$modes": {
        "default": "default",
        "cds-g0": "cds-g0",
        "cds-g10": "cds-g10",
        "cds-g90": "cds-g90",
        "cds-g100": "cds-g100"
      }
    },
    "xyz": {
      "$value": "{transformed.x} {transformed.y} {transformed.z}"
    },
    "xk(t)": {
      "$value": "{transformed.x} {transformed.k-themed}"
    }
  }
}