{
  "link": {
    "standalone": {
      "padding": {
        "vertical": {
          "$type": "dimension",
          "$value": "4",
          "unit": "px",
          "$modes": {
            "default": "4",
            "cds-g0": "0",
            "cds-g10": "0",
            "cds-g90": "0",
            "cds-g100": "0"
          }
        }
      },
      "typography": {
        "font-size": {
          "small": {
            "$type": "font-size",
            "$value": "0.8125",
            "unit": "rem",
            "$modes": {
              "default": "0.8125",
              "cds-g0": "{carbon.typography.helperText01.font-size}",
              "cds-g10": "{carbon.typography.helperText01.font-size}",
              "cds-g90": "{carbon.typography.helperText01.font-size}",
              "cds-g100": "{carbon.typography.helperText01.font-size}"
            }
          },
          "medium": {
            "$type": "font-size",
            "$value": "0.875",
            "unit": "rem"
          },
          "large": {
            "$type": "font-size",
            "$value": "1",
            "unit": "rem"
          }
        },
        "letter-spacing": {
          "small": {
            "$type": "letter-spacing",
            "$value": 0,
            "unit": "px",
            "$modes": {
              "default": 0,
              "cds-g0": "{carbon.typography.helperText01.letter-spacing}",
              "cds-g10": "{carbon.typography.helperText01.letter-spacing}",
              "cds-g90": "{carbon.typography.helperText01.letter-spacing}",
              "cds-g100": "{carbon.typography.helperText01.letter-spacing}"
            }
          },
          "medium": {
            "$type": "letter-spacing",
            "$value": 0,
            "unit": "px",
            "$modes": {
              "default": 0,
              "cds-g0": "{carbon.typography.bodyCompact01.letter-spacing}",
              "cds-g10": "{carbon.typography.bodyCompact01.letter-spacing}",
              "cds-g90": "{carbon.typography.bodyCompact01.letter-spacing}",
              "cds-g100": "{carbon.typography.bodyCompact01.letter-spacing}"
            }
          },
          "large": {
            "$type": "letter-spacing",
            "$value": 0,
            "unit": "px",
            "$modes": {
              "default": 0,
              "cds-g0": "{carbon.typography.bodyCompact02.letter-spacing}",
              "cds-g10": "{carbon.typography.bodyCompact02.letter-spacing}",
              "cds-g90": "{carbon.typography.bodyCompact02.letter-spacing}",
              "cds-g100": "{carbon.typography.bodyCompact02.letter-spacing}"
            }
          }
        },
        "line-height": {
          "small": {
            "$type": "number",
            "$value": 1.231,
            "comment": "16px ~= 1.231",
            "$modes": {
              "default": 1.231,
              "cds-g0": "{carbon.typography.helperText01.line-height}",
              "cds-g10": "{carbon.typography.helperText01.line-height}",
              "cds-g90": "{carbon.typography.helperText01.line-height}",
              "cds-g100": "{carbon.typography.helperText01.line-height}"
            }
          },
          "medium": {
            "$type": "number",
            "$value": 1.143,
            "comment": "16px ~= 1.143",
            "$modes": {
              "default": 1.143,
              "cds-g0": "{carbon.typography.bodyCompact01.line-height}",
              "cds-g10": "{carbon.typography.bodyCompact01.line-height}",
              "cds-g90": "{carbon.typography.bodyCompact01.line-height}",
              "cds-g100": "{carbon.typography.bodyCompact01.line-height}"
            }
          },
          "large": {
            "$type": "number",
            "$value": 1.5,
            "comment": "24px = 1.5",
            "$modes": {
              "default": 1.5,
              "cds-g0": "{carbon.typography.bodyCompact02.line-height}",
              "cds-g10": "{carbon.typography.bodyCompact02.line-height}",
              "cds-g90": "{carbon.typography.bodyCompact02.line-height}",
              "cds-g100": "{carbon.typography.bodyCompact02.line-height}"
            }
          }
        }
      },
      "icon": {
        "size": {
          "small": {
            "$type": "dimension",
            "$value": "0.75",
            "unit": "rem",
            "$modes": {
              "default": "0.75",
              "cds-g0": "1",
              "cds-g10": "1",
              "cds-g90": "1",
              "cds-g100": "1"
            }
          },
          "medium": {
            "$type": "dimension",
            "$value": "1",
            "unit": "rem"
          },
          "large": {
            "$type": "dimension",
            "$value": "1.5",
            "unit": "rem",
            "$modes": {
              "default": "1.5",
              "cds-g0": "1.25",
              "cds-g10": "1.25",
              "cds-g90": "1.25",
              "cds-g100": "1.25"
            }
          }
        }
      },
      "underline": {
        "color": {
          "primary": {
            "hover": {
              "$type": "color",
              "$value": "#4e81e8",
              "$modes": {
                "default": "#4e81e8",
                "cds-g0": "{carbon.themes.white.linkPrimaryHover}",
                "cds-g10": "{carbon.themes.g10.linkPrimaryHover}",
                "cds-g90": "{carbon.themes.g90.linkPrimaryHover}",
                "cds-g100": "{carbon.themes.g100.linkPrimaryHover}"
              }
            },
            "active": {
              "$type": "color",
              "$value": "#396ed6",
              "$modes": {
                "default": "#396ed6",
                "cds-g0": "{carbon.themes.white.linkPrimary}",
                "cds-g10": "{carbon.themes.g10.linkPrimary}",
                "cds-g90": "{carbon.themes.g90.linkPrimary}",
                "cds-g100": "{carbon.themes.g100.linkPrimary}"
              }
            }
          },
          "secondary": {
            "hover": {
              "$type": "color",
              "$value": "#4d4d4f",
              "$modes": {
                "default": "#4d4d4f",
                "cds-g0": "{color.foreground.primary}",
                "cds-g10": "{color.foreground.primary}",
                "cds-g90": "{color.foreground.primary}",
                "cds-g100": "{color.foreground.primary}"
              }
            },
            "active": {
              "$type": "color",
              "$value": "#6e7075",
              "$modes": {
                "default": "#6e7075",
                "cds-g0": "{color.foreground.faint}",
                "cds-g10": "{color.foreground.faint}",
                "cds-g90": "{color.foreground.faint}",
                "cds-g100": "{color.foreground.faint}"
              }
            }
          }
        }
      },
      "surface": {
        "color": {
          "primary": {
            "active": {
              "$type": "color",
              "$value": "var(--token-color-surface-action)",
              "$modes": {
                "default": "var(--token-color-surface-action)",
                "cds-g0": "transparent",
                "cds-g10": "transparent",
                "cds-g90": "transparent",
                "cds-g100": "transparent"
              }
            }
          },
          "secondary": {
            "active": {
              "$type": "color",
              "$value": "var(--token-color-surface-interactive-active)",
              "$modes": {
                "default": "var(--token-color-surface-interactive-active)",
                "cds-g0": "transparent",
                "cds-g10": "transparent",
                "cds-g90": "transparent",
                "cds-g100": "transparent"
              }
            }
          }
        }
      }
    }
  }
}