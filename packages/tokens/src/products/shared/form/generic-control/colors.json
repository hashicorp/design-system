{
  "form": {
    "control": {
      "base": {
        "foreground": {
          "value-color": {
            "$type": "color",
            "$value": "{color.foreground.strong}",
            "group": "components"
          },
          "placeholder-color": {
            "$type": "color",
            "$value": "{color.foreground.faint}",
            "group": "components"
          }
        },
        "surface-color": {
          "default": {
            "$type": "color",
            "$value": "{color.surface.interactive}",
            "group": "components"
          },
          "hover": {
            "$type": "color",
            "$value": "{color.surface.interactive-hover}",
            "group": "components"
          }
        },
        "border-color": {
          "default": {
            "$type": "color",
            "$value": "{color.palette.neutral-400}",
            "group": "components"
          },
          "hover": {
            "$type": "color",
            "$value": "{color.palette.neutral-500}",
            "group": "components"
          }
        }
      },
      "checked": {
        "foreground-color": {
          "$type": "color",
          "$value": "{color.foreground.high-contrast}",
          "group": "components"
        },
        "surface-color": {
          "default": {
            "$type": "color",
            "$value": "{color.palette.blue-200}",
            "group": "components"
          },
          "hover": {
            "$type": "color",
            "$value": "{color.palette.blue-300}",
            "group": "components"
          }
        },
        "border-color": {
          "default": {
            "$type": "color",
            "$value": "{color.palette.blue-300}",
            "group": "components"
          },
          "hover": {
            "$type": "color",
            "$value": "{color.palette.blue-400}",
            "group": "components"
          }
        }
      },
      "invalid": {
        "border-color": {
          "default": {
            "$type": "color",
            "$value": "{color.palette.red-300}",
            "group": "components"
          },
          "hover": {
            "$type": "color",
            "$value": "{color.palette.red-400}",
            "group": "components"
          }
        }
      },
      "readonly": {
        "foreground-color": {
          "$type": "color",
          "$value": "{color.foreground.primary}",
          "group": "components"
        },
        "surface-color": {
          "$type": "color",
          "$value": "{color.surface.strong}",
          "group": "components"
        },
        "border-color": {
          "$type": "color",
          "$value": "{color.border.faint}",
          "group": "components"
        }
      },
      "disabled": {
        "foreground-color": {
          "$type": "color",
          "$value": "{color.foreground.disabled}",
          "group": "components"
        },
        "surface-color": {
          "$type": "color",
          "$value": "{color.surface.interactive-disabled}",
          "group": "components"
        },
        "border-color": {
          "$type": "color",
          "$value": "{color.border.primary}",
          "group": "components"
        }
      }
    }
  }
}
