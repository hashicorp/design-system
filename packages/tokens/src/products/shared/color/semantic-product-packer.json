{
  "color": {
    "packer": {
      "brand": {
        "$type": "color",
        "$value": "{color.palette.packer-brand}",
        "group": "branding"
      },
      "foreground": {
        "$type": "color",
        "$value": "{color.palette.packer-500}",
        "group": "branding"
      },
      "surface": {
        "$type": "color",
        "$value": "{color.palette.packer-50}",
        "group": "branding"
      },
      "border": {
        "$type": "color",
        "$value": "{color.palette.packer-100}",
        "group": "branding"
      },
      "gradient": {
        "primary": {
          "start": {
            "$type": "color",
            "$value": "{color.palette.packer-100}",
            "group": "branding"
          },
          "stop": {
            "$type": "color",
            "$value": "{color.palette.packer-200}",
            "group": "branding"
          }
        },
        "faint": {
          "start": {
            "$type": "color",
            "$value": "#F3FCFF",
            "group": "branding",
            "comment": "this is the 'packer-50' value at 25% opacity on white"
          },
          "stop": {
            "$type": "color",
            "$value": "{color.palette.packer-50}",
            "group": "branding"
          }
        }
      }
    }
  }
}