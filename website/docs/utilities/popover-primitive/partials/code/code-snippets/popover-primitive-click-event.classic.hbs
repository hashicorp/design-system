<Hds::PopoverPrimitive @enableClickEvents={{true}} as |PP|>
  <div id="container" {{PP.setupPrimitiveContainer}}>
    <button id="toggle" type="button" {{PP.setupPrimitiveToggle}}>
      toggle
    </button>
    <div id="popover" {{PP.setupPrimitivePopover}}>
      popover content
    </div>
  </div>
</Hds::PopoverPrimitive>