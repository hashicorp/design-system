{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "all"
        }
      ]
    },
    {
      "source": "/testing(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "none"
        }
      ]
    }
  ],
  "framework": "ember",
  "rewrites": [{ "source": "/(.*)", "destination": "/error.html" }]
}
