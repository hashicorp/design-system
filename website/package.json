{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for website goes here",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashicorp/design-system.git",
    "directory": "website"
  },
  "license": "MPL-2.0",
  "author": "HashiCorp Design Systems <design-systems@hashicorp.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "generate-changelog-markdown-files": "node lib/changelog/generate-changelog-markdown-files.mjs",
    "generate-component-changelog-entries": "node lib/changelog/generate-component-changelog-entries.mjs",
    "build": "yarn build:packages && ember build --environment=production",
    "build:packages": "yarn workspace @hashicorp/design-system-components build",
    "lint": "concurrently \"yarn:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"yarn:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:css": "stylelint \"app/styles/**/*.scss\"",
    "lint:css:fix": "concurrently \"yarn:lint:css --fix\"",
    "lint:spell:docs": "cspell --no-progress --show-suggestions --show-context \"docs/**/*.md\"",
    "start": "yarn build:packages && ember serve",
    "start:prember": "yarn build:packages && PREMBER=true ember serve",
    "algolia:index": "node docs-scraper/index-website-content.mjs",
    "test": "concurrently \"yarn:lint\" \"yarn:test:*\" --names \"lint,test:\"",
    "test:ember": "yarn build:packages && ember test",
    "test:ember-a11y": "yarn build:packages && ENABLE_A11Y_AUDIT=true ember test --server"
  },
